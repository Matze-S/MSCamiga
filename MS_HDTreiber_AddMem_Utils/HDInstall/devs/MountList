
/* example mountlist entry for a 20MB HardDisk */

DH0:        FileSystem = L:FastFileSystem
            StackSize = 1000
            Priority = 10
            GlobVec = -1

            Device = harddisk.device
            Unit  = 0
            Flags = 0

            Surfaces = 4
            BlocksPerTrack = 17
            Reserved = 2
            PreAlloc = 0
            Interleave = 0
            LowCyl = 0
            HighCyl = 614
            Buffers = 30
            BufMemType = 1
            MaxTransfer = 0x7FFFFFFF
            Mask = 0xFFFFFFFE
            BootPri = 1                 /* Should not be greater than 5 */
            DosType = 0x444F5301
            Mount = 1
#
